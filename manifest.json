{
  "manifest_version": 3,
  "name": "NonLocalLogic",
  "version": "1.1",
  "description": "NonLocalLogic",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://www.thelocal.se/*"],
  "action": {
    "default_popup": "popup.html"
  }
}